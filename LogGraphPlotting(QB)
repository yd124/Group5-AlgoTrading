import matplotlib.pyplot as plt
import numpy as np
import math
from datetime import datetime
qb = QuantBook()

pair = ['WIRE', 'ALG']
P = pair[0]
Q = pair[1]
eP = qb.AddEquity(P)
eQ = qb.AddEquity(Q)
start_time = datetime(2018,1,2)
end_time = datetime(2021,4,15)
history = qb.History(qb.Securities.Keys, start_time, end_time, Resolution.Hour)

hP = np.log(history.loc[P]['close'].to_numpy())
hQ = np.log(history.loc[Q]['close'].to_numpy())

logprice = plt.plot(hP, hQ)
plt.xlabel(P)
plt.ylabel(Q)
